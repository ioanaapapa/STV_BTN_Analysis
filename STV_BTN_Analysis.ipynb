    "        SDs.append(stdev(values_array))\n",
    "        SEs.append(stdev(values_array)/math.sqrt(5))\n",
    "    sum_SD = 0\n",
    "    sum_SE = 0\n",
    "    for i in range (0,(len(new_df)-1)):\n",
    "        sum_SD+=SDs[i]**2\n",
    "        sum_SE+=SEs[i]**2\n",
    "    SEs.append(math.sqrt(sum_SE))\n",
    "    new_df[new_column_name]=SEs"
