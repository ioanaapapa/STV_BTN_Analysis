    "#where no_of_residues is per monomer\n",
    "def average_per_residue (df):\n",
    "    average_values_per_residue = []\n",
    "    no_of_residues = int((len(df)+1)/5)\n",
    "    for i in range (0, no_of_residues):\n",
    "        average_values_per_residue.append(mean([df.iloc[i][\"Average Per Chain\"], df.iloc[i+no_of_residues[i+no_of_residues]][\"Average Per Chain\"], df.iloc[(i+2*no_of_residues)][\"Average Per Chain\"], df.iloc[(i+3*no_of_residues)][\"Average Per Chain\"],df.iloc[(i+4*no_of_residues)][\"Average Per Chain\"]]))\n",
    "    return average_values_per_residue"
   ]
